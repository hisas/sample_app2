- provide(:title, 'All users')
%h1 All users
= form_with(url: "users/search", method: :get, skip_enforcing_utf8: true) do
  = label_tag :name, "Name"
  = text_field_tag :name, '', placeholder: "Please enter name"
  = submit_tag  'Search'
= paginate @users
%ul.users
  = render @users
= paginate @users
